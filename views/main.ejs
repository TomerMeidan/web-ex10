<!DOCTYPE html>
<html>
  <head>
    <title>Main Page</title>
  </head>
  <body>
    <h1>Welcome, <%= username %>!</h1>
    <h2>User Table</h2>
    <table>
      <tr>
        <th>ID</th>
        <th>Username</th>
      </tr>
      <% users.forEach(user => { %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
